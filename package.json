{
  "name": "wattpad-to-pdf",
  "module": "index.js",
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "bun index.js",
    "api": "bun --watch index.js --ignore client/**",
    "client": "cd client && bun run dev",
    "server-install": "bun install",
    "client-install": "cd client && bun install",
    "install-all": "concurrently \"bun run server-install\" \"bun run client-install\"",
    "dev": "concurrently \"bun run api\" \"bun run client\""
  },
  "dependencies": {
    "axios": "1.5.1",
    "cheerio": "1.0.0-rc.12",
    "concurrently": "8.2.1",
    "cors": "2.8.5",
    "dompurify": "^3.0.6",
    "express": "4.18.2",
    "html2canvas": "^1.4.1",
    "morgan": "1.10.0",
    "resolve": "1.22.8"
  }
}